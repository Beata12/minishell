int main() {
char* gjs_debug_topics = getenv("GJS_DEBUG_TOPICS");
char* language = getenv("LANGUAGE");
char* lc_time = getenv("LC_TIME");
char* user = getenv("USER");
char* xdg_session_type = getenv("XDG_SESSION_TYPE");
char* shlvl = getenv("SHLVL");
char* xdg_cache_home = getenv("XDG_CACHE_HOME");
char* home = getenv("HOME");
char* desktop_session = getenv("DESKTOP_SESSION");
char* gio_launched_desktop_file = getenv("GIO_LAUNCHED_DESKTOP_FILE");
char* gtk_modules = getenv("GTK_MODULES");
char* xdg_seat_path = getenv("XDG_SEAT_PATH");

    printf("GJS_DEBUG_TOPICS: %s\n", gjs_debug_topics);
    printf("LANGUAGE: %s\n", language);
    printf("LC_TIME: %s\n", lc_time);
    printf("USER: %s\n", user);
    printf("XDG_SESSION_TYPE: %s\n", xdg_session_type);
    printf("SHLVL: %s\n", shlvl);
    printf("XDG_CACHE_HOME: %s\n", xdg_cache_home);
    printf("HOME: %s\n", home);
    printf("DESKTOP_SESSION: %s\n", desktop_session);
    printf("GIO_LAUNCHED_DESKTOP_FILE: %s\n", gio_launched_desktop_file);
    printf("GTK_MODULES: %s\n", gtk_modules);
    printf("XDG_SEAT_PATH: %s\n", xdg_seat_path);

    return 0;

}
